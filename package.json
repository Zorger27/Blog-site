  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": ["src/**/*.{vue,js,ts}"],
    "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel"
  },
  "overrides": [
    {
      "files": [
        "**/__tests__/*.{j,t}s?(x)",
