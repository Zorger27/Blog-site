    "vue": "^3.2.13",
    "vue-class-component": "^8.0.0-0",
    "vue-i18n": "^9.1.0",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^3.0.0",
    "@types/jest": "^27.0.1",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-e2e-cypress": "^5.0.8",
